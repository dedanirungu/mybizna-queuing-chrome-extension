{
  "manifest_version": 3,
  "name": "MyBizna Queuing",
  "version": "1.0",
  "description": "Opens a specific URL in an iframe.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}

